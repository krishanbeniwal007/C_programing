##Find the Time 1
#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    float r,t;
    scanf("%f",&r);
    t=100/r;
    printf("%.2f",t);
    

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
##Hello Me 1
