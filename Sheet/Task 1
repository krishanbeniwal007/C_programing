Q1
#include<stdio.h>
int main()
{
    int a;
    float b;
    char c;
    double d;
    scanf("%d",&a);
    printf("Value is %d\n",a);
    scanf("%f",&b);
    printf("Value is %f\n",b);
    scanf("%c",&c);
    printf("Value is %c\n",c);
    scanf("%lf",&d);
    printf("Value is %lf",d);
    return 0;

}

Q2
#include<stdio.h>
int main()
{
    int a,b;
    printf("Enter 2 values:\n");
    scanf("%d",&a);
    scanf("%d",&b);
    printf("sum of two no.%d",a+b);
}

Q3
#include<stdio.h>
int main()
{
    int a,b;
    printf("Enter 2 values:\n");
    scanf("%d",&a);
    scanf("%d",&b);
    printf("sum of two no.%d\n",a+b);
    printf("sum of two no.%d\n",a-b);
    printf("sum of two no.%d\n",a/b);
    printf("sum of two no.%d\n",a%b);
    printf("sum of two no.%d",a*b);
}

Q4
#include<stdio.h>
int main()
{
    int a,b,c;
    printf("Enter 2 values:\n");
    scanf("%d",&a);
    scanf("%d",&b);
    printf("length %d\n",a);
    printf("breath %d\n",b);
    c=2*(a+b);
    printf("perimeter of rectangle %d",c);
}

Q5
#include<stdio.h>
int main()
{
    int a,b,c;
    printf("Enter 2 values:\n");
    scanf("%d",&a);
    scanf("%d",&b);
    printf("length %d\n",a);
    printf("breath %d\n",b);
    c=(a*b);
    printf("area of rectangle %d",c);
}

Q6
#include<stdio.h>
int main()
{
    float r;
    printf("Enter radius");
    scanf("%f",&r);
    printf("Radius is %f\n",r);
    printf("diameter is %f\n",(r*2));
    printf("Circumference is %f\n",(3.14*2*r));
    printf("Area is %f\n",(3.14*r*r));
}

Q7
#include<stdio.h>
int main()
{
    float r;
    printf("Enter length");
    scanf("%f",&r);
    printf("length in cm  is %f\n",r);
    printf("length in m is %f\n",(0.1*r));
    printf("length in km is %f\n",(0.000001*r));
}

Q8
#include<stdio.h>
int main()
{
    float r;
    printf("Enter temperature");
    scanf("%f",&r);
    printf("temperature in celcius is %f\n",r);
    printf("temperature in farenhite is %f\n",((r*(9/5))+32));
}

Q9
#include<stdio.h>
int main()
{
    float r;
    printf("Enter temperature");
    scanf("%f",&r);
    printf("temperature in farenhite is %f\n",r);
    printf("temperature in celcius is %f\n",((r-32)*(5/9)));
}
Q10 
#include<stdio.h>
int main()
{
    int  r,y,w,d;
    printf("Enter days");
    scanf("%d",&r);
    y=r/365;
    w=(r%365)/7;
    d=(r%365)%7;
    printf("year %d\nweeks %d\ndays %d\n",y,w,d);
}

Q11
#include<stdio.h>
#include<math.h>
int main()
{
    int  r,y;
    printf("Enter two numbers\n");
    scanf("%d",&r);
    printf("First number is %d\n",r);
    scanf("%d",&y);
    printf("Second number is %d\n",y);
    printf("no. 1 power to no. 2 is %lf\n",(pow(r,y)));
}

