#include<math.h>
int main()
{
    int  r;
    printf("Enter angle of equalateral triangle\n");
    scanf("%d",&r);
    printf("side is %d\n",r);
    printf("area of triangle is %lf\n",((1.732*r*r)));
}

Q16
#include<stdio.h>
#include<math.h>
int main()
{
    int  a,b,c,d,e;
    printf("Enter score of five students\n");
    scanf("%d %d %d %d %d",&a,&b,&c,&d,&e);
    printf("scores are %d\n%d\n%d\n%d\n%d\n",a,b,c,d,e);
    printf("Avrage of the students is %d\n",((a+b+c+d+e)/5));
    printf("percentage is %d\n",(a+b+c+d+e)/5);
}
Q17
#include<stdio.h>
#include<math.h>
int main()
{
    float p,r,t;
    printf("Enter principle, rate, time to find simple interest\n");
    scanf("%f\n%f\n%f\n",&p,&r,&t);
    printf("principle %f\n rate %f\n time%f\n",p,r,t);
    printf("Simple interesr is %f",(p*(1+r*t)));
}

Q18
#include<stdio.h>
#include<math.h>
int main()
{
    float p,r,t,n,x,y,z,c;
    printf("Enter principle, rate, time, number of times interest applied to find simple interest\n");
    scanf("%f%f%f%f",&p,&r,&t,&n);
    printf("principle %f\n rate %f\n time%f\n number of times interest applied%f\n",p,r,t,n);
    x=n*t;
    y=1+(r/n);
    c=pow(y,x);
    z=p*c;
    printf("Compound interwst %lf\n",z);
}
