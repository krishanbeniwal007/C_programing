}

Q13
#include<stdio.h>
#include<math.h>
int main()
{
    int  r,y;
    printf("Enter two Angle\n");
    scanf("%d",&r);
    printf("First Angle is %d\n",r);
    scanf("%d",&y);
    printf("Second Angle is %d\n",y);
    printf("Third Angle is %d degrees\n",(180-r-y));
}

Q14
#include<stdio.h>
#include<math.h>
int main()
{
    int  r,y;
    printf("Enter base and height of a triangle\n");
    scanf("%d",&r);
    printf("base is %d\n",r);
    scanf("%d",&y);
    printf("height is %d\n",y);
    printf("area of triangle is %lf\n",((r*y)/2));
}

Q15
